    "prettier": "^3.4.2",
    "react-test-renderer": "18.3.1",
    "reactotron-react-native": "^5.1.11",
    "reactotron-redux": "^3.1.10",
    "rollup": "^4.29.1",
    "syncpack": "^12.4.0",
    "ts-jest": "^29.2.5",
    "ts-node": "10.9.2",
    "tsc-files": "^1.1.4",
