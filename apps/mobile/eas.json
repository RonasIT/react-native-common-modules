{
  "build": {
    "development": {
      "channel": "development",
      "distribution": "internal",
      "ios": {
        "distribution": "store"
      },
      "env": {
        "APP_ENV": "development"
      }
    },
    "debug": {
      "extends": "development",
      "developmentClient": true,
      "ios": {
        "distribution": "internal"
      }
    },
    "staging": {
      "channel": "staging",
      "distribution": "store",
      "env": {
        "APP_ENV": "staging"
      }
    },
    "production": {
      "channel": "production",
      "distribution": "store",
      "env": {
        "APP_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
